{
  "name": "video-music-lyrics",
  "version": "1.0.0",
  "description": "✨A browser plug-in that allows CC subtitles from mainstream video websites on the market to be displayed outside the browser window✨",
  "private": true,
  "scripts": {
    "watch": "node ./node_modules/webpack/bin/webpack.js --mode=development --watch --config config/webpack.config.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --mode=production --config config/webpack.config.js"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^6.11.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.10.1",
    "webpack": "^5.102.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.10.0"
  },
  "keywords": [
    "youtube",
    "lyrics",
    "subtitles",
    "cc",
    "desktop",
    "browser-extension"
  ],
  "author": "Cherry",
  "license": "MIT"
}